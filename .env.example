# Conteúdo do arquivo /meu-rumo/meu-rumo/.env.example

# Exemplo de configuração de variáveis de ambiente para o Meu Rumo

# URL do banco de dados
DATABASE_URL=mysql://usuario:senha@localhost:3306/meu_rumo

# Chave secreta para autenticação
JWT_SECRET=suachavesecreta

# Configurações do servidor
PORT=3000

# Configurações da API de IA
AI_API_KEY=sua_chave_api_ia

# Configurações de e-mail
EMAIL_HOST=smtp.seuprovedor.com
EMAIL_PORT=587
EMAIL_USER=seuemail@provedor.com
EMAIL_PASS=suasenha

# Configurações do gateway de pagamento
PAYMENT_GATEWAY_URL=https://api.pagseguro.com
PAYMENT_API_KEY=sua_chave_api_pagseguro

# Outras configurações
NODE_ENV=development